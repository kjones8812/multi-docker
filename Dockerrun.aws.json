{
  "AWSEBDockerrunVersion":2,
  "containerDefinitions":[
    {
      "name": "client",
      "image": "kjones8812/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "kjones8812/multi-server",
      "hostname": "api",
      "essential": false,
      "memory":128
    },
    {
      "name": "worker",
      "image": "kjones8812/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory":128
    },
    {
      "name": "nginx",
      "image":"kjones8812/nginx" ,
      "hostname": "nginx",
      "essential": true,
      "memory":128
      "portmappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client","server"]
    }



  ]
}
